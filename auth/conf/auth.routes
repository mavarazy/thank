GET         /social/:provider                com.clemble.loveit.auth.controllers.SocialAuthController.authenticate(provider)

POST        /signUp                          com.clemble.loveit.auth.controllers.SignUpController.submit
POST        /signIn                          com.clemble.loveit.auth.controllers.SignInController.submit

POST        /password/forgot                 com.clemble.loveit.auth.controllers.ForgotPasswordController.submit
POST        /password/reset/:token           com.clemble.loveit.auth.controllers.ResetPasswordController.submit(token: java.util.UUID)
POST        /password/change                 com.clemble.loveit.auth.controllers.ChangePasswordController.submit

GET         /logout                          com.clemble.loveit.auth.controllers.LogoutController.logout()